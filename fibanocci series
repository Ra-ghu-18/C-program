#include<stdio.h>
int main(){
    int num1,num2,num3,n,i;
    num1=0,num2=1;
    num3=num1+num2;
    printf("the no of terms: ");
    scanf("%d",&n);
    printf("the series is: %d ,%d, ",num1,num2);
    for (i = 3; i <= n; i++) {
    printf("%d, ", num3);
    num1 = num2;
    num2 = num3;
    num3 = num1 + num2;
}
return 0;
}
